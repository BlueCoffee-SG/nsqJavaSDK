<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="NSQ-Client Trace Suite">
    <test name="consumer and producer " parallel="methods" thread-count="4">
        <classes>
            <class name="it.youzan.nsq.client.ITProducerTrace">
                <methods>
                    <include name="publishTrace"/>
                </methods>
            </class>
            <class name="it.youzan.nsq.client.ITConsumerTrace"/>
        </classes>
    </test>
</suite>